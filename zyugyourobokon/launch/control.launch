<launch>

<node pkg="tf" type="static_transform_publisher" name="hokuyo_broadcaster" args="0 0.15 0.03  1.57079632679 0 0  base_link laser 100" />
<node pkg="urg_node" type="urg_node" name="urg_node" output="screen" >
    <param name="serial_port" value="/dev/ttyACM2"/>
</node>

<node pkg="joy" type="joy_node" name="joy_node">
<param name="dev" type="string" value="/dev/input/js1"/>
</node>

<!--joy_twist-->
<node pkg="control" type="control" name="control_node" output="screen" />

<!--twist_arduino-->
 <node name="tf_twist" type="tf_twist" pkg="tf_twist" >
 <param name="speed" value="15" />
 </node>
<node
 pkg="rosserial_python"
 type="serial_node.py"
 name="serial_node">

<param name="port" value="/dev/ttyACM3"/>
<param name="baud" value="57600"/>
</node>

<node
 pkg="rosserial_python"
 type="serial_node.py"
 name="serial_node2">

<param name="port" value="/dev/ttyACM1"/>
<param name="baud" value="57600"/>
</node>


<node pkg="odometry" type="odometry_node" name="odometry_node" output="screen" />


<node pkg="tf_listener" type="tf_listener_node" name="tf_listener" output="screen" />



</launch>

